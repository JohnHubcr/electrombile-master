package com.zenchn.electrombile.entity;

/**
 * 版本更新
 */
public class AppStoreInfo {

    private String verCode;// 当前服务器版本号
    private String verDownloadLink;// 下载链接
    private String fileMd5;// MD5值
    private String updateInfo;// 更新内容
    private Boolean isMust;// 是否强制升级

    public String getVerCode() {
        return verCode;
    }

    public void setVerCode(String verCode) {
        this.verCode = verCode;
    }

    public String getVerDownloadLink() {
        return verDownloadLink;
    }

    public void setVerDownloadLink(String verDownloadLink) {
        this.verDownloadLink = verDownloadLink;
    }

    public String getFileMd5() {
        return fileMd5;
    }

    public void setFileMd5(String fileMd5) {
        this.fileMd5 = fileMd5;
    }

    public String getUpdateInfo() {
        return updateInfo;
    }

    public void setUpdateInfo(String updateInfo) {
        this.updateInfo = updateInfo;
    }

    public Boolean getIsMust() {
        return isMust;
    }

    public void setIsMust(Boolean isMust) {
        this.isMust = isMust;
    }

    @Override
    public String toString() {
        return "AppStoreInfo{" +
                "verCode='" + verCode + '\'' +
                ", verDownloadLink='" + verDownloadLink + '\'' +
                ", fileMd5='" + fileMd5 + '\'' +
                ", updateInfo='" + updateInfo + '\'' +
                ", isMust=" + isMust +
                '}';
    }
}
